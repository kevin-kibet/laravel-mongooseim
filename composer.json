{
  "name": "kevin-kibet/laravel-mongooseim",
  "description": "Mongoose API Wrapper for Laravel/lumen",
  "license": "MIT",
  "authors": [
    {
      "name": "kevin-kibet",
      "email": "kevynkybet@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "guzzlehttp/guzzle": "^6.3",
    "illuminate/support": "^5.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.3",
    "orchestra/testbench": "~3.0"
  },
  "autoload": {
    "psr-4": {
      "MongooseIm\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MongooseIm\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "MongooseIm\\Providers\\MongooseImServiceProvider"
      ],
      "aliases": {
        "MongooseIm": "MongooseIm\\Facades\\MongooseIm"
      }
    }
  }
}
